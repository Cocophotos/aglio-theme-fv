{
  "name": "aglio-theme-fv",
  "version": "1.0.0",
  "description": "Theme for the Aglio API Blueprint renderer",
  "main": "lib/main.js",
  "scripts": {
    "build": "coffeelint src/*.coffee && coffee -o lib -c src",
    "prepublish": "npm run build",
    "postinstall": "node scripts/setup-cache.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cocophotos/aglio-theme-fv"
  },
  "keywords": [
    "api",
    "blueprint",
    "protagonist",
    "snowcrash",
    "html",
    "parse",
    "markdown"
  ],
  "author": "Corentin Ribeyre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cocophotos/aglio-theme-fv/issues"
  },
  "homepage": "https://github.com/Cocophotos/aglio-theme-fv/tree/master",
  "dependencies": {
    "coffee-script": "^1.8.0",
    "highlight.js": "^8.4.0",
    "jade": "^1.8.2",
    "less": "^2.1.2",
    "markdown-it": "^4.3.0",
    "markdown-it-anchor": "^2.1.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^1.0.0",
    "markdown-it-emoji": "^1.0.0",
    "moment": "^2.8.4",
    "stylus": "^0.51.1"
  },
  "devDependencies": {
    "coffeelint": "^1.7.1"
  }
}
